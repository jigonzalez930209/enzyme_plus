{
  "name": "enzyme_plus",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "author": {
    "name": "José Iván González Jorge",
    "email": "jigonzalez930209@gmail.com",
    "url": "https://github.com/jigonzalez930209"
  },
  "email": "jigonzalez930209@gmail.com",
  "homepage": "https://github.com/jigonzalez930209/enzyme_plus/releases",
  "license": "MIT",
  "description": "Enzyme Plus is a web application that allows users to perform enzyme kinetics experiments based on stochastic models of S + E --> ES --> EP --> E + P governed by k_1, k_2, k_3 an its inverse k-1 k-2 k-3 and track on the time evolution of the concentrations of S, E, ES, EP and P.",
  "build": {
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "appId": "com.jigonzalez930209.enzyme_plus",
    "asar": true,
    "publish": {
      "provider": "github",
      "owner": "jigonzalez930209",
      "repo": "enzyme_plus",
      "releaseType": "release"
    },
    "productName": "enzyme_plus",
    "win": {
      "icon": "public/logo.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "artifactName": "${productName}-Windows-${version}-Setup-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": true
    },
    "mac": {
      "target": [
        "dmg",
        "zip",
        "pkg"
      ],
      "artifactName": "${productName}-Mac-${version}-Setup.${ext}"
    },
    "linux": {
      "icon": "build/icons",
      "category": "Office",
      "maintainer": "Jose Ivan <jigonzalez@gmail.com>",
      "description": "Enzyme Plus is a web application that allows users to perform enzyme kinetics experiments based on stochastic models of S + E --> ES --> EP --> E + P governed by k_1, k_2, k_3 an its inverse k-1 k-2 k-3 and track on the time evolution of the concentrations of S, E, ES, EP and P.",
      "target": [
        "deb",
        "AppImage"
      ],
      "artifactName": "${productName}-Linux-${version}-Setup.${ext}"
    },
    "extraResources": [
      {
        "from": "dist",
        "to": "dist"
      }
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "ELECTRON_DISABLE_SANDBOX=1 electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:gh-pages": "GITHUB_PAGES=1 electron-vite build",
    "wasm:build": "wasm-pack build wasm --release --target web --out-dir ../src/wasm --out-name enzyme_sim",
    "wasm:dev": "wasm-pack build wasm --dev --target web --out-dir ../src/wasm --out-name enzyme_sim"
  },
  "dependencies": {
    "@mdx-js/react": "^3.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.11",
    "@types/plotly.js": "^2.35.2",
    "@types/react-plotly.js": "^2.6.3",
    "big.js": "^7.0.1",
    "decimal.js": "^10.6.0",
    "lucide-react": "^0.539.0",
    "mdx-mermaid": "^2.0.3",
    "mermaid": "^11.10.0",
    "plotly.js": "^3.1.0",
    "plotly.js-basic-dist": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "tailwindcss": "^4.1.11",
    "usehooks-ts": "^3.1.1",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^4.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "main": "dist-electron/main.cjs"
}
